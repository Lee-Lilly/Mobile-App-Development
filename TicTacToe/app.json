{
  "expo": {
    "name": "TicTacToe",
    "slug": "TicTacToe",
    "version": "1.0.0",
    "description" :"A Simple TicTacToe Game for two",
    "platforms": [
      "ios",
      "android",
      "web"
    ], 
    "orientation": "portrait",
    "icon": "./assets/tic-tac-toe.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.example.tictactoe",
      "versionCode": 1
    }
  }
}
